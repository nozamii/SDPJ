package com.example.DB.RealDB;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity
public class News {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private long nid;
    private String title;
    private String descriptions;

    public News(long i, String bn, String  p) {
        this.title = bn;
        this.descriptions = p;

    }

    public News(String bn, String  p) {
        this.title = bn;
        this.descriptions = p;

    }

    public News(String  p) {
        this.descriptions = p;
    }

    public News() {

    }

    public void setId(long proid) {
        this.nid = proid;
    }

    public long getId() {
        return nid;
    }

    public void setTitle(String pron) {
        this.title = pron;
    }

    public String getTitle() {
        return title;
    }

    public void setDesc(String  pron) {
        this.descriptions = pron;
    }

    public String  getDesc() {
        return descriptions;
    }
}
